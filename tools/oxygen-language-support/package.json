{
  "name": "oxygen-language-support",
  "displayName": "Oxygen for VSCodium",
  "description": "Oxygen language support for VSCodium",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "oxygen-language-support.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "oxy",
        "aliases": ["Oxygen", "oxygen"],
        "extensions": [
          ".oxy"
        ]
      }
    ],
    "grammars": [
      {
        "language": "oxy",
        "scopeName": "source.oxy",
        "path": "./syntaxes/oxy.tmLanguage."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.52.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
